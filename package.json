{
  "name": "tessin-mini",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch webpack --watch server.js server.js",
    "build": "rimraf build && webpack --config webpack/webpack.config.prod.server.js && webpack --config webpack/webpack.config.prod.client.js",
    "build:prod": "cross-env NODE_ENV=production yarn run -s build",
    "build-client": "rimraf build/client && webpack --config webpack/webpack.config.prod.client.js --profile --json > compilation-stats.json",
    "build-client:prod": "cross-env NODE_ENV=production yarn run -s build-client",
    "prod": "yarn run build:prod -s && node server-prod.js"
  },
